        "@types/iframe-resizer": "^3.5.13",
        "@types/jasmine": "~5.1.4",
        "@types/node": "^18.18.0",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "browser-sync": "^3.0.0",
        "eslint": "^8.53.0",
        "jasmine-core": "~5.1.2",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
